<h1> Sign In </h1>
<%= form_tag '/signin', method: 'post' %>
  <%= label_tag :name, "Your Name:" %>
  <%= collection_select :user, :name, User.all, :name, :name, prompt: "Please select your username" %>
  <%= label_tag "Your Password" %>
  <%= password_field_tag :password %>
  <%= submit_tag "Sign In" %>
