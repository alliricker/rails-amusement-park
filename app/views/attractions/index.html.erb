<% if @user.admin %>

<%= link_to "New Attraction", new_attraction_path %><br>
<% @attractions.each do |attraction| %>
<%= link_to "Show #{attraction.name}", attraction_path(attraction.id) %><br>

<% end %>

<% else %>

<% @attractions.each do |attraction| %>
<%= link_to "Go on #{attraction.name}", attraction_path(attraction.id) %>
<%= attraction.tickets %> <br>
<% end %>
<% end %>
